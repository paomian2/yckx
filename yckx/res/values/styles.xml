    <!-- POPwindown每个选项布局样式 -->
    <style name="news_more_popup">
        <item name="android:layout_width">45dp</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:textColor">#FFF</item>
        <item name="android:textStyle">bold</item>
        <item name="android:drawablePadding">40dip</item>
        <item name="android:background">@drawable/leftmenu_item_bg</item>
    </style>
    
    
    
     <!-- 高德地图上选择显示 -->
    <style name="showSelect">
        <item name="android:layout_width">45dp</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:drawablePadding">40dip</item>
    </style>
    
    
    
    <style name="popup_animation_style">
        <item name="android:windowEnterAnimation">@anim/anim_popup_wind_enter</item>  
    </style>
    
     <!-- 默认按钮背景色(默认与按下) -->
    <color name="defbtnbg_n">#FF009ACD</color>
    <color name="defbtnbg_p">#AA009ACD</color>
   
    <!-- 软件盘的样式 -->
    <style name="soft_keyboard_style">
         <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">50dp</item>
          <item name="android:gravity">center</item>
           <item name="android:background">@drawable/defbtnbg_cir</item>
    </style>
    
    
  <style name="dialog" parent="@android:style/Theme.Dialog"><!--name是我们在使用时要用到的资源的标志，parent是指当前的样式所继承的父类样式--> 
     <item name="android:windowFrame">@null</item>  
    <item name="android:windowIsFloating">true</item>  
    <item name="android:windowIsTranslucent">false</item>  
    <item name="android:windowNoTitle">true</item><!--隐藏标题栏--> 
    <item name="android:windowBackground">@drawable/bg_empty</item><!--这儿也很重要啊，我这儿用了一张透明的.9.png的图，当然用#00000000也是可以的，否则的话这儿出来后有一个黑色的背景--> 
    <item name="android:backgroundDimEnabled">false</item>  
  </style>  
  
  
   <style name="dialog2" parent="@android:style/Theme.Dialog"><!--name是我们在使用时要用到的资源的标志，parent是指当前的样式所继承的父类样式--> 
     <item name="android:windowFrame">@null</item>  
    <item name="android:windowIsFloating">true</item>  
    <item name="android:windowIsTranslucent">false</item>  
    <item name="android:windowNoTitle">true</item><!--隐藏标题栏--> 
    <item name="android:windowBackground">@color/white</item><!--这儿也很重要啊，我这儿用了一张透明的.9.png的图，当然用#00000000也是可以的，否则的话这儿出来后有一个黑色的背景--> 
    <item name="android:backgroundDimEnabled">false</item>  
  </style>  
  
    <style name="dialog3" parent="@android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item> 
        <item name="android:windowBackground">@color/white</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
   
  
  
    <!-- 洗护师订单处理 -->
    <style name="washer_order">
        <item name="android:background">@color/white</item>
        <item name="android:textColor">@color/red</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:drawablePadding">20.0dip</item>
